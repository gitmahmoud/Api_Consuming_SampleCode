</head>
<body>
    <div class="navbar navbar-expand-lg navbar-light bg-light">

                @Html.ActionLink("Test Dafater APi", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })

            <div class="collapse navbar-collapse">
                <ul class="navbar-nav mr-auto">

                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                           Customer
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            @Html.ActionLink("Create New Customer", "Add", "Customer", new { id = "" }, new { @class = "dropdown-item" })
                            @Html.ActionLink("Load Customer مبيعات الرحاب", "Edit", "Customer", new { id = "مبيعات الرحاب" }, new { @class = "dropdown-item" })
