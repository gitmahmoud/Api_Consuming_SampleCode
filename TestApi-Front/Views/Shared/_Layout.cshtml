                        </div>
                    </li>

                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Sales Invoice
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            @Html.ActionLink("Add Sales Invoice", "Add", "SalesInvoice", new { id = "" }, new { @class = "dropdown-item" })
                            @Html.ActionLink("Edit INV00001", "Edit", "SalesInvoice", new { id = "INV00001" }, new { @class = "dropdown-item" })
                            @Html.ActionLink("List", "Index", "SalesInvoice", new { id = "" }, new { @class = "dropdown-item" })
                        </div>
                    </li>

                        @*<li>@Html.ActionLink("About", "About", "Home")</li>
    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                </ul>
            </div>
        </div>


    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer></footer>
    </div>


</body>
</html>
